    "migrate:prod": "prisma migrate deploy --schema=prisma/prod/schema.prisma",
    "generate:prod": "prisma generate --schema=prisma/prod/schema.prisma",
    "build": "concurrently \"tsc\" \"npm run generate:prod\""
  },
